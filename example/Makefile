#Makefile
all: simple asio twisted netty idleconnection maxconnection

simple:
	cd simple && $(MAKE)

asio:
	cd asio && $(MAKE)

twisted:
	cd twisted && $(MAKE)

netty:
	cd netty && $(MAKE)

idleconnection:
	cd idleconnection && $(MAKE)

maxconnection:
	cd maxconnection && $(MAKE)

.PHONY: all simple asio twisted netty idleconnection maxconnection
