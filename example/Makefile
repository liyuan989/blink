	cd netty && $(MAKE)

idleconnection:
	cd idleconnection && $(MAKE)

maxconnection:
	cd maxconnection && $(MAKE)

roundtrip:
	cd roundtrip && $(MAKE)

hub:
	cd hub && $(MAKE)

.PHONY: all simple asio twisted netty idleconnection maxconnection roundtrip hub
